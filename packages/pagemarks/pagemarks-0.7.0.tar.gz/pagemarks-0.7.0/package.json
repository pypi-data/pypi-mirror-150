{
    "name": "pagemarks",
    "version": "0.7.0",
    "description": "Free, git-backed, self-hosted bookmarks",
    "keywords": [
        "bookmark",
        "bookmarks",
        "bookmark-manager",
        "bookmarks-manager",
        "self-hosted",
        "github-pages",
        "gitlab-pages"
    ],
    "homepage": "https://pagemarks.org/",
    "bugs": {
        "url": "https://gitlab.com/barfuin/pagemarks/-/issues"
    },
    "license": "GPL-3.0-only",
    "author": {
        "name": "Barfuin"
    },
    "repository": "https://gitlab.com/barfuin/pagemarks",
    "scripts": {
        "test": "c8 --exclude \\\"pagemarks/js/vendor/**\\\" --exclude \\\"test/**\\\" --reports-dir ./build/coverage --reporter text --reporter html mocha --require mocha-suppress-logs",
        "test-xml": "mocha --require mocha-suppress-logs --reporter xunit --reporter-options output=build/TEST-mocha-test-results.xml,suiteName=\"JavaScript Tests\""
    },
    "type": "module",
    "dependencies": {
        "@yaireo/tagify": "4.10.0",
        "bootstrap": "5.1.3",
        "bootstrap-icons": "1.8.0",
        "bootswatch": "5.1.3",
        "buffer": "6.0.3",
        "crypto-es": "1.2.7",
        "jquery": "3.6.0",
        "shufflejs": "6.0.0"
    },
    "devDependencies": {
        "c8": "^7.11.2",
        "chai": "^4.3.6",
        "mocha": "^9.2.2",
        "mocha-suppress-logs": "~0.3.1",
        "sass": "^1.49.7"
    }
}
