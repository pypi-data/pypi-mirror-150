{
  "name": "jupyterlab-openbis",
  "version": "0.3.0",
  "description": "JupyterLab openBIS extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "openBIS"
  ],
  "homepage": "https://sissource.ethz.ch/sispub/jupyterlab-openbis",
  "bugs": {
    "url": "https://sissource.ethz.ch/sispub/jupyterlab-openbis/issues"
  },
  "license": "Apache-2.0",
  "author": "Antti Luomi | ID SIS | ETH ZÃ¼rich",
  "files": [
    "js/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf,tsx}",
    "css/*.css"
  ],
  "main": "js/index.js",
  "types": "js/index.d.ts",
  "style": "css/index.css",
  "repository": {
    "type": "git",
    "url": "https://sissource.ethz.ch/sispub/jupyterlab-openbis.git"
  },
  "scripts": {
    "build": "jlpm run build:ts && jlpm run build:scss && jlpm run build:labextension",
    "build:ts": "tsc",
    "build:scss": "node-sass scss -o css",
    "build:labextension": "jupyter labextension build .",
    "clean": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:lib": "rimraf js css tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyter-openbis/labextension",
    "watch": "jlpm run build && run-p watch:ts watch:scss watch:labextension",
    "watch:ts": "tsc -w",
    "watch:scss": "node-sass --watch scss -o css",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/notebook": "^3.0.0",
    "@jupyterlab/apputils": "^3.0.0",
    "@jupyterlab/docregistry": "^3.0.0",
    "@lumino/disposable": "^1.4.0",
    "immer": "^6.0.9",
    "@types/classnames": "^2.2.6"
  },
  "devDependencies": {
    "rimraf": "2.6.1",
    "typescript": "4.1.3",
    "node-sass": "4.14.1",
    "@jupyterlab/builder": "^3.0.0",
    "npm-run-all": "4.1.5"
  },
  "sideEffects": [
    "css/*.css"
  ],
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab-openbis/labextension/",
    "_build": {
      "load": "static/remoteEntry.9b1c00156b99d1e051af.js",
      "extension": "./extension",
      "style": "./style"
    }
  }
}
